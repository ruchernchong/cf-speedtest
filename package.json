{
  "name": "cf-speedtest",
  "version": "0.2.3",
  "description": "CLI tool for testing internet speed using Cloudflare's network",
  "keywords": [
    "speedtest",
    "cloudflare",
    "cli",
    "network",
    "bandwidth",
    "latency",
    "internet-speed",
    "performance",
    "testing"
  ],
  "author": "Ru Chern Chong <hello@ruchern.dev>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruchernchong/cf-speedtest.git"
  },
  "homepage": "https://github.com/ruchernchong/cf-speedtest#readme",
  "bugs": {
    "url": "https://github.com/ruchernchong/cf-speedtest/issues"
  },
  "type": "module",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "files": ["dist"],
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "cf-speedtest": "dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "check": "biome check src/**",
    "release": "changeset publish"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.29.4",
    "@vitest/coverage-v8": "3.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=18"
  }
}
